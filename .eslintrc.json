{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "google"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": [
    "html"
  ],
  "ignorePatterns": [
    "dist/"
  ],
  "rules": {
    "no-var": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off"
  }
}
